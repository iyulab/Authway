{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/*.{css,scss,js,png,jpg,jpeg,gif,svg,ico,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/login",
      "rewrite": "/index.html"
    },
    {
      "route": "/consent",
      "rewrite": "/index.html"
    },
    {
      "route": "/register",
      "rewrite": "/index.html"
    },
    {
      "route": "/verify-email",
      "rewrite": "/index.html"
    },
    {
      "route": "/resend-verification",
      "rewrite": "/index.html"
    },
    {
      "route": "/forgot-password",
      "rewrite": "/index.html"
    },
    {
      "route": "/reset-password",
      "rewrite": "/index.html"
    },
    {
      "route": "/error",
      "rewrite": "/index.html"
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
